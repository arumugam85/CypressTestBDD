{
  "name": "DemoCypressProject",
  "version": "1.0.0",
  "description": "Demo Automation Project with Cypress",
  "main": "index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "pretest": "node ./cypress/reporting/delete-results.js",
    "report": "node ./cypress/reporting/cucumber-report.js",
    "cy:tags": "./node_modules/.bin/cypress-tags run",
    "cy:run": "cypress run --browser chrome",
    "cy:open": "cypress open",
    "cypress:report": "generate-mochawesome-report",
    "report:merge": "mochawesome-merge --reportDir cypress/results/json > cypress/results/mochawesome-bundle.json",
    "report:generate": "marge cypress/results/mochawesome-bundle.json -o cypress/reports/html",
    "clean": "rm -rf screenshots",
    "reporttest": "node cypress/createHtmlReports.js",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "start": "ng serve",
    "ngtest": "ng test",
    "build": "ng build --prod",
    "ng": "ng",
    "e2etest": "npm start & cypress run"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "cucumber-html-reporter",
    "html report",
    "json to html"
  ],
  "author": "Arumugam",
  "license": "ISC",
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "results/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "assert": "^2.0.0",
    "chalk": "^2.4.2",
    "cypress": "^5.6.0",
    "cypress-cucumber-preprocessor": "4.0.0",
    "cypress-wait-until": "1.7.1",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "js-base64": "^2.3.2",
    "jsonfile": "^5.0.0",
    "lodash": "^4.17.11",
    "log4js": "^6.3.0",
    "multiple-cucumber-html-reporter": "1.18.0",
    "node-emoji": "^1.10.0",
    "open": "^6.4.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "cosmiconfig": "^7.0.0",
    "cucumber-expressions": "^8.3.0",
    "cucumber-html-reporter": "^5.3.0",
    "cucumber-tag-expressions": "^2.0.3",
    "cypress-downloadfile": "^1.2.0",
    "cypress-file-upload": "^4.1.1",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-mochawesome-reporter": "^1.3.0",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-select-tests": "^1.5.9",
    "eslint": "7.12.1",
    "eslint-plugin-cypress": "2.11.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.2",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^7.0.0",
    "start-server-and-test": "^1.7.11",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
